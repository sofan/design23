<div class="footer">
    <div>
        En footer med lite text.
    </div>
    <div class="social">
        {% for social in pages["_meta"].meta.social %}
        <a target="blank" href="{{ social.url }}" title="{{ social.title }}" role="button">
            {% if social.iconunicode %}
                {{ social.iconunicode }}
            {% else %}
                <i class="{{ social.icon }}" aria-hidden="true"></i>
                <span class="sr-only">{{ social.title }}</span>
            {% endif %}
        </a>
        {% endfor %}
    </div>
</div>

<script src="{{ theme_url }}/../shared//js/modernizr-3.3.1-custom.min.js" ></script>
<script src="{{ theme_url }}/../shared//js/utils.js" ></script>
<script src="{{ theme_url }}/../shared//js/responsive-navbar.js" ></script>
